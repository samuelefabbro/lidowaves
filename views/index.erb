			<div class="col-md-6">
				<h5><%= @h_onda_inizio %> m</h5>
			</div>
			<div class="col-md-6">
				<img src="img/<%= @x %>.png">
			</div>
		</div>
	</div>
	<div class="col-md-3">
		<h5>Onda max</h5>
		<h2><%= @h_onda_max %> m</h2>
