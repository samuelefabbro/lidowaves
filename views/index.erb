	<div class="col-md-3">
		<h5>Direzione vento</h5>
		<h2><%= @dir_vento %> GN</h2>
		<img src="img/freccia.png" style="-webkit-transform:rotate(<%= @dir_vento %>deg);">
	</div>
	<div class="col-md-3">
		<h5>Velocit√† Vento media</h5>
		<h2><%= @vento %> m/s</h2>
		<div class="row">
			<div class="col-md-6">
				<h5><%= @vento_inizio %> m/s</h5>
			</div>
			<div class="col-md-6">
				<img src="img/<%= @z %>.png">
			</div>
		</div>
